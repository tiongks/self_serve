<h1>Listing sub_categories</h1>

<table>
  <tr>
    <th>Categor</th>
    <th>Sub-category name</th>
    <th>Active</th>
    <th>Effective date</th>
    <th>Last effective date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sub_categories.each do |sub_category| %>
  <tr>
    <td><%= sub_category.categor_id %></td>
    <td><%= sub_category.sub_category_name %></td>
    <td><%= sub_category.active %></td>
    <td><%= sub_category.effective_date %></td>
    <td><%= sub_category.last_effective_date %></td>
    <td><%= link_to 'Show', sub_category %></td>
    <td><%= link_to 'Edit', edit_sub_category_path(sub_category) %></td>
    <td><%= link_to 'Destroy', sub_category, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sub category', new_sub_category_path %>
